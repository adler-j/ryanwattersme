---
insearch: false
---

{
    "siteContent": [
    	{% for article in site.articles %}
    		{% unless article.url == "/articles/" %}
        {"articleTitle": "{{article.title}}",
        "articleImage": "{{article.main_image}}",
        "articleUrl": "{{article.url}}",
        "articleTags": "{% for tag in article.tags %}{{tag}}{% if forloop.rindex0 > 0 %}, {% endif %}{% endfor %}"
    }{% if forloop.rindex0 > 0 %}, {% endif %}
    	{% endunless %}
    	{% endfor %}]
}