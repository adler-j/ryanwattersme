---
layout: null
insearch: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for item in site.tutorials | sort: 'series_no' %}
	{% unless item.published == false %}
	<url>
		<loc>{{ site.url }}{{ item.url }}</loc>
		{% if item.lastmod == null %}
		<lastmod>{{item.publishdate}}</lastmod>
		{% else %}
		<lastmod>{{item.lastmod}}</lastmod>
		{% endif %}		
		<changefreq>weekly</changefreq>
    <priority>1.0</priority>
	</url>
	{% endunless %}
{% endfor %}

{% for item in site.articles | sort: 'publishdate' | reverse %}
	{% unless item.published == false %}
	<url>
		<loc>{{ site.url }}{{ item.url }}</loc>
		{% if item.lastmod == null %}
		<lastmod>{{item.publishdate}}</lastmod>
		{% else %}
		<lastmod>{{item.lastmod}}</lastmod>
		{% endif %}		
		<changefreq>weekly</changefreq>
    <priority>0.8</priority>
	</url>
	{% endunless %}
{% endfor %}
{% for item in site.pages | sort: 'publishdate' | reverse %}
	{% unless item.published == false %}
	<url>
		<loc>{{ site.url }}{{ item.url }}</loc>
		{% if item.lastmod == null %}
		<lastmod>{{item.publishdate}}</lastmod>
		{% else %}
		<lastmod>{{item.lastmod}}</lastmod>
		{% endif %}		
		<changefreq>weekly</changefreq>
    <priority>0.5</priority>
	</url>
	{% endunless %}
{% endfor %}
</urlset>